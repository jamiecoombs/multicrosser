<h1><%= @parsed_crossword['name'] %></h1>
<p>
  <%= Time.at(@parsed_crossword['date']/1000).to_date.strftime("%A %-d %B") %>
  <% if @parsed_crossword['creator']['name'] != "" %>
     - Set by <a href="<%= @parsed_crossword['creator']['webUrl'] %>"><%= @parsed_crossword['creator']['name'] %></a>
  <%- end -%>
</p>

<%= content_tag(:div, '', class: 'js-crossword', data: {
  crossword: @crossword,
  'crossword-identifier': crossword_identifier,
  room: params[:room],
  author: params[:participant]
}) %>

<%= javascript_pack_tag "application", "data-turbolinks-track": "reload" %>


